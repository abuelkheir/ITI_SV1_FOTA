{
    "version": 2,
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/api",
            "dest": "/api/server.js"
        },
        {
            "src": "/api/upload",
            "dest": "api/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "public/index.html"
        }
    ]
}